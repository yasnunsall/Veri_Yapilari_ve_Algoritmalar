         [16, 21, 11, 8, 12, 22]
[16, 21, 11]        ||        [8, 12, 16]
[16, 21][11]        ||        [8, 12][16]
[16][21][11]        ||        [8][12][16]
[16][21, 11]        ||        [8, 12][16]
[11, 16, 21]        ||        [8, 12, 16]
         [8, 11, 12, 16, 21, 22]
         
Big O = O(n*log(n))
